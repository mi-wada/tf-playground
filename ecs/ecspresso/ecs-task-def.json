{
  "containerDefinitions": [
    {
      "cpu": 0,
      "environment": [
        {
          "name": "GUNICORN_CMD_ARGS",
          "value": "--bind=0.0.0.0:80"
        }
      ],
      "essential": true,
      "image": "kennethreitz/httpbin:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/httpbin",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "httpbin"
        }
      },
      "name": "httpbin",
      "portMappings": [
        {
          "appProtocol": "",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "versionConsistency": ""
    }
  ],
  "cpu": "256",
  "executionRoleArn": "{{ tfstate `aws_iam_role.ecs_task_execution.arn` }}",
  "taskRoleArn": "{{ tfstate `aws_iam_role.ecs_task.arn` }}",
  "family": "httpbin",
  "ipcMode": "",
  "memory": "512",
  "networkMode": "awsvpc",
  "pidMode": "",
  "requiresCompatibilities": [
    "FARGATE"
  ]
}
